{
  "name": "preevy",
  "version": "0.0.2",
  "description": "Quickly deploy preview environments to the cloud!",
  "author": "Livecycle",
  "bin": {
    "preevy": "./bin/run"
  },
  "homepage": "https://github.com/livecycle/preevy",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "livecycle/preevy",
  "files": [
    "/bin",
    "/dist",
    "/yarn.lock",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@aws-sdk/client-lightsail": "^3.271.0",
    "@aws-sdk/client-s3": "^3.271.0",
    "@aws-sdk/client-sts": "^3.289.0",
    "@aws-sdk/util-waiter": "^3.271.0",
    "@livecycle/compose-tunnel-agent": "0.0.2",
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.3.0",
    "@types/ssh2": "^1.11.7",
    "cli-color": "^2.0.3",
    "glob": "^9.2.1",
    "inquirer": "^8.0.0",
    "iter-tools-es": "^7.5.1",
    "lodash": "^4.17.21",
    "lodash.mapvalues": "^4.6.0",
    "node-forge": "^1.3.1",
    "node-ssh": "^13.0.1",
    "p-retry": "^4.6.2",
    "read-package-json": "^6.0.0",
    "remove": "^0.1.5",
    "rimraf": "^4.4.0",
    "shell-escape": "^0.2.0",
    "tar": "^6.1.13",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.4",
    "@types/chai": "^4",
    "@types/inquirer": "^8.0.0",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^9.0.0",
    "@types/node": "18",
    "@types/node-forge": "^1.3.1",
    "@types/shell-escape": "^0.2.1",
    "@types/ssh2-streams": "^0.1.9",
    "@types/tar": "^6.1.4",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "chai": "^4",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.4.3",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2",
    "wtfnode": "^0.9.1"
  },
  "oclif": {
    "bin": "preevy",
    "dirname": "preevy",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "hooks": {
      "init": [
        "./dist/hooks/init/sync-logging"
      ]
    },
    "topicSeparator": " "
  },
  "scripts": {
    "copy-statics-to-dist": "cp -R static dist",
    "build": "shx rm -rf dist && tsc -b && yarn copy-statics-to-dist",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "cli": "./bin/dev",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/livecycle/preevy/issues",
  "keywords": [
    "preview"
  ],
  "types": "dist/index.d.ts"
}
